        "def feet2meters(feet):\n",
        "  if type(feet) is str:\n",
        "    print(\"Error: the input is a string\")\n",
        "    return\n",
        "  meters = feet * 0.3048\n",
        "  return meters\n",
        "\n",
        "def feet2meters_str(feet):\n",
        "  meters = feet2meters(feet)\n",
        "  answer_str = \"{:.4f} feet = {:.4f} meters\".format(feet,meters)\n",
        "  return answer_str\n",
        "\n",
        "def feet2meters_printout(feet):\n",
        "  answer_str = feet2meters_str(feet)\n",
        "  print(answer_str)\n",
        "  return\n",
        "\n",
        "\n"
      ]
    },
